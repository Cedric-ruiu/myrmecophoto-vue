    "@prisma/client": "^6.15.0",
    "@types/image-size": "^0.8.0",
    "@unocss/nuxt": "^66.5.0",
    "@unocss/preset-icons": "^66.5.0",
    "@unocss/preset-typography": "^66.5.0",
    "@unocss/preset-web-fonts": "^66.5.0",
