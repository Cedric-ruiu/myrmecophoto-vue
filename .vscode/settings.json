{
  "css.validate": false,
  "scss.validate": false,
  "prettier.enable": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.fixAll.stylelint": "explicit"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "stylelint.validate": ["css", "scss", "vue", "html"],
  "cSpell.words": [
    "Camponotus",
    "Capitatus",
    "Dimage",
    "Formicidae",
    "fuliginosus",
    "gyne",
    "Konica",
    "Lasius",
    "Messor",
    "Myrmeco",
    "myrmécologique",
    "myrmécologiques",
    "Myrmecophoto",
    "Myrmica",
    "netlify",
    "nuxt",
    "pallidula",
    "Pheidole",
    "prisma",
    "Raynox",
    "rubra",
    "unocss"
  ],
  "[scss, vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
